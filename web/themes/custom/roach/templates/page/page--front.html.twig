{#
/**
 * @file
 * Default theme implementation to present front page layout.
 */
#}
{% set bem_block = 'app' %}
<div class="{{ bem_block }}">
  {% include '@roach/include/header.html.twig' %}
  {% include '@roach/include/header-mobile.html.twig' %}

  <main{{ main_layout_attributes.addClass(bem_block ~ '__content') }}>
    {{ page.sidebar_first }}
    {% include '@roach/include/frontpage.html.twig' %}
    {{ page.sidebar_second }}
  </main>

  {% include '@roach/include/footer.html.twig' %}
  {% include '@roach/include/mobile-navigation.html.twig' %}
</div>
